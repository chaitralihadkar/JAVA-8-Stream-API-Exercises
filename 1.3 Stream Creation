package com.bootcamp.StreamAPI.StreamCreation;

import java.util.List;
import java.util.stream.Collectors;
import java.util.stream.Stream;

public class Streamofdoubleliterals
{
    public static void main(String[] args)
    {
        Stream<Double> nums = Stream.of(1.3, 3.5, 4.3, 6.1, 2.9);

        List<Double> doubles = nums.collect(Collectors.toList());

        System.out.println(doubles);
    }
}
