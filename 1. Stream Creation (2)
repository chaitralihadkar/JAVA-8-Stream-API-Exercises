package com.bootcamp.StreamAPI.StreamCreation;

import java.util.Arrays;
import java.util.stream.Stream;

public class streamofevenno
{
    public static void main(String[] args)
    {
        Integer[] array = {2,3,4,5,6,7,8};

        Stream<Integer> nums = Arrays.stream(array);

        Stream<Integer> evennums = nums.filter(a -> a%2!=0);

        evennums.forEach(System.out::println);
    }
}
