package com.bootcamp.StreamAPI.IntermediateOperations;

import java.util.Arrays;
import java.util.Comparator;
import java.util.List;
import java.util.stream.Stream;

public class reversealphabeticalorder
{
    public static void main(String[] args)
    {
        List<String> names = Arrays.asList("tarak","zara","jay", "ketan","lara","arya");

        Stream<String> data = names.stream();

        Stream<String> reversesortednames = data.sorted(Comparator.reverseOrder());

        reversesortednames.forEach(n -> System.out.println(n));
    }
}
