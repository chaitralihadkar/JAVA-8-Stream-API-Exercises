package com.bootcamp.StreamAPI.TerminalOperations;

import java.util.stream.Stream;

public class count
{
    public static void main(String[] args)
    {
        Stream<Integer> list = Stream.of(4,1,6,7,2,8,9);

        long count = list.filter(n -> n%2!=0)
                .count();

        System.out.println(count);
    }
}
